{
  "providers": {
    "auth": {
      "address": "http://localhost:8080/",
      "authentication": "Basic",
      "endpoints": {
        "login": "/auth/login",
        "logout": "/auth/logout"
      },
      "sessionBearer": "X-Auth-Token"
    },
    "resources": [
      {
        "name": "photo-endpoint",
        "address": "http://localhost:8080/photo",
        "format": "application/json"
      },
      {
        "name": "project-endpoint",
        "address": "http://localhost:8080/project",
        "format": "application/json"
      }
    ]
  }
}
